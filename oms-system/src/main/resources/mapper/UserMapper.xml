<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.iecas.mapper.UserMapper">

    <select id="selectByName" parameterType="String" resultType="com.iecas.domain.User">
        SELECT id, `name`, name_zh, `password` FROM sys_user WHERE `name` = #{name}
    </select>


</mapper>